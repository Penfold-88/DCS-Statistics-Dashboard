# Copy this file to .env and fill in your actual values
# Environment variables take precedence over config.ini

# FTP Configuration
FTP_HOST=your.ftp.server.com
FTP_USER=your_ftp_username
FTP_PASSWORD=your_ftp_password
# Use secure FTP connection (FTPS) - set to true for secure connection
FTP_SECURE=true

# Path Configuration
LOCAL_FOLDER=/path/to/your/dcs/logs
REMOTE_FOLDER=/public_html/data

# Upload Configuration
THROTTLE_SECONDS=1
DISPLAY_COUNTDOWN=true